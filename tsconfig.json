{
    "compilerOptions": {
      /* Base Options: */
      "esModuleInterop": true,
      "skipLibCheck": true,
      "target": "es2022",
      "allowJs": true,
      "resolveJsonModule": true,
      "moduleDetection": "force",
      "isolatedModules": true,
  
      /* Strictness */
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "checkJs": true,
  
      /* Bundled projects */
      "lib": ["dom", "dom.iterable", "ES2022"],
      "noEmit": true,
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "jsx": "preserve",
  
      "incremental": true,
  
      /* Path Aliases */
      "baseUrl": "./",
      // "paths": {
      //   "~/*": ["./src/*"],
      //   "~fivemDb": ["./src/db/fivem"],
      //   "~schema": ["./src/db"],

      //   "~print": ["./src/print"],
      // }
    },
    "include": [
     "debug-CSS.js",
"debugCSS.css",
"debugCSS.js",
     
    //   "src/**/*.ts",
    //   "rollup.config.js"
    
     

    ],
    "exclude": ["node_modules",  "build", "src/debugCSS.css"]
  }
  